class WhileNonStatic 
{
	public void laptop()
	{
		System.out.println("hp");
	}
	public static void main(String[] args) 
	{
		System.out.println("main starts...");
		WhileNonStatic w1 = new WhileNonStatic();
		int a=1;
		while (a<=4)
		{
			w1.laptop();
		}
		System.out.println("main ends...");
	}
}
