class MainClass3 
{
	public void display(WildAnimals w)
	{
		w.roam();
		w.hunt();
		w.eat();
	}
	public static void main(String[] args) 
	{
		MainClass3 m1 = new MainClass3();
		m1.display(new Lion());
		m1.display(new Tiger());
		m1.display(new Cheetah());
	}
}
